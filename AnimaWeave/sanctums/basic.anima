-- types
Signal
UUID
Int
Bool
String
Prompt {
    id basic.String
    name basic.String
    content basic.String
}
--

-- nodes
Start {
    mode Concurrent
    in {}
    out {
        signal basic.Signal
        execution_id basic.UUID
    }
}

GetTimestamp {
    mode Concurrent
    in {
        trigger basic.Signal
    }
    out {
        timestamp basic.Int
        done basic.Signal
    }
}

IsEven {
    mode Concurrent
    in {
        number basic.Int
        trigger basic.Signal
    }
    out {
        result basic.Bool
        done basic.Signal
    }
}

FormatNumber {
    mode Concurrent
    in {
        number basic.Int
        trigger basic.Signal
    }
    out {
        formatted basic.String
        done basic.Signal
    }
}

CreatePrompt {
    mode Concurrent
    in {
        name basic.String
        content basic.String
        trigger basic.Signal AND
    }
    out {
        prompt basic.Prompt
        done basic.Signal
    }
}
-- 